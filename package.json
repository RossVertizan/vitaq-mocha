{
  "name": "vitaqai-mocha",
  "version": "10.1.1",
  "description": "Fork of Mocha to enable VitaqAI to run in the Mocha environment",
  "keywords": [
    "mocha",
    "vitaq"
  ],
  "author": "Ross Addinall <ross@vertizan.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RossVertizan/vitaq-mocha/vitaq-mocha.git"
  },
  "bugs": {
    "url": "https://github.com/RossVertizan/vitaq-mocha/issues"
  },
  "homepage": "https://github.com/RossVertizan/vitaq-mocha",
  "logo": "https://vitaq.io/wp-content/uploads/2020/10/Vitaq-new-logo-small.png",
  "notifyLogo": "https://vitaq.io/wp-content/uploads/2020/10/Vitaq-new-logo-small.png",
  "bin": {
    "mocha": "./bin/mocha.js",
    "_mocha": "./bin/_mocha"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "prepublishOnly": "nps test clean build",
    "start": "nps",
    "test": "nps test",
    "test.node.unit": "nps test.node.unit",
    "test.node.unit_runner": "nps test.node.unit_runner",
    "test.node.integration": "nps test.node.integration",
    "test:smoke": "node ./bin/mocha --no-config test/smoke/smoke.spec.js",
    "test:integration-color": "node ./bin/mocha test/integration/color.spec.js"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "browser-stdout": "1.3.1",
    "chokidar": "3.5.3",
    "debug": "4.3.4",
    "diff": "5.0.0",
    "escape-string-regexp": "4.0.0",
    "find-up": "5.0.0",
    "glob": "7.2.0",
